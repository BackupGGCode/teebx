#Description: TeeBX - BeagleBone Black (eglibc)

# select init
SDECFGSET_INIT='busybox'

# architecture
SDECFGSET_ARCH='arm'
SDECFGSET_ARM_ENDIANESS='el'
SDECFGSET_ARM_OPT='cortex-a8'
SDECFGSET_ARM_ABI='gnueabi'
SDECFGSET_ARM_FLOAT_ABI='hard'
SDECFGSET_ARM_FPU='neon'
SDECFGSET_ARM_BOARDDEF='omap2plus'
SDECFGSET_ARM_THUMB=0
SDECFGSET_SOFTFLOAT=0

# firmware type
SDECFGSET_IMAGE='ubootimage'
# U-Boot board configuration options
#
# Leaving _UBOOT_TARGETBOARD blank/unset u-boot build will default to SDECFGSET_ARM_BOARDDEF
SDECFGSET_PKG_UBOOT_TARGETBOARD='am335x_evm'
# IPL binary name
#SDECFGSET_PKG_UBOOT_IPLBIN=''
# SPL binary name
SDECFGSET_PKG_UBOOT_SPLBIN='MLO'

# kernel options
SDECFGSET_PKG_LINUX_DTBS='am335x-boneblack.dtb'
SDECFGSET_PKG_LINUX_IMG_FORMAT='zImage'
SDECFGSET_PKG_LINUX_IMG_NAME=''
SDECFGSET_PKG_LINUX_CUSTCONFIG_FILE="target/$target/linux.config"
SDECFGSET_PKG_LINUX_CONFIG_STYLE='none'
SDECFGSET_PKG_LINUX_CUSTCONFIG='1'
#SDECFGSET_PKG_LINUX_USE26MODTOOLS='1'
#SDECFGSET_PKG_LINUX_ACPI_INITRD='0'
SDECFGSET_PKG_LINUX_KEEP_SRC='1'

# busybox package options
SDECFGSET_PKG_BUSYBOX_SYMLINKS='1'

# optimize for...
SDECFGSET_OPT='speed'
SDECFGSET_CREATE_DOCS=0
SDECFG_DEBUG='0'
# no package creation
SDECFGSET_PKGFILE_TYPE='none'
# libc + gcc settings
SDECFGSET_LIBC='eglibc'
SDECFGSET_PKG_GCC_JAVA='0'

# do not execute rebuild 'paranoia' stage
SDECFGSET_DO_REBUILD_STAGE=0
# force cross compiling
SDECFGSET_CROSSBUILD=1
# enable native language support
SDECFGSET_DISABLE_NLS=0
