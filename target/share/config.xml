<?xml version="1.0"?>
<!-- TeeBX default system configuration -->
<!-- $Id$ -->
<teebx>
	<version>0.1</version>
	<lastchange></lastchange>
	<system>
		<hostname>TeeBX</hostname>
		<domain>local</domain>
		<dnsserver>8.8.4.4</dnsserver>
		<username>admin</username>
		<password>teebx</password>
		<time-update-interval>4-hours</time-update-interval>
		<timeservers>pool.ntp.org</timeservers>
		<webgui>
			<protocol>http</protocol>
		</webgui>
		<storage>
			<mountroot>/offload/opt/storage</mountroot>
			<fsmounts>
				<media_01>
					<uuid>520B-C01C</uuid>
					<comment>Storage01</comment>
					<label>TeeBX_s01</label>
					<filesystem>vfat</filesystem>
					<active>0</active>
				</media_01>
			</fsmounts>
			<services>
				<astmedia>
					<fsmount>media_01</fsmount>
				</astmedia>
				<astdb>
				</astdb>
				<astcdr>
				</astcdr>
			</services>
		</storage>
		<earlyshellcmd></earlyshellcmd>
		<shellcmd></shellcmd>
		<disable-update-check/>
		<notes></notes>
	</system>
	<interfaces>
		<lan>
			<dhcp/>
			<if>eth0</if>
			<ipaddr>192.168.1.2</ipaddr>
			<subnet>24</subnet>
			<gateway>192.168.1.1</gateway>
		</lan>
	</interfaces>
	<dialplan/>
</teebx>
